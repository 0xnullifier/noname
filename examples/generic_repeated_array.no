fn init_arr(const LEFT: Field, const RIGHT: Field) -> [Field; LEFT * 2 + RIGHT] {
    let arr = [0; (LEFT * 2) + RIGHT];
    return arr;
}

fn main(pub public_input: Field) -> [Field; 7] {
    // [Field; 3 * 2 + 1]
    let mut arr = init_arr(3, 1);
    for ii in 0..7 {
        arr[ii] = public_input;
    }
    return arr;
}