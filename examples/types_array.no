struct Thing {
    x: Field,
    y: Field,
}

fn main(pub x: Field, pub y: Field) {
    let thing1 = Thing {
        x: 1,
        y: 2,
    };
    let thing2 = Thing {
        x: 3,
        y: 4,
    };
    let things = [thing1, thing2];
    
    assert_eq(things[0].x, x);
    assert_eq(things[1].y, y);
}
